<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <AssemblyName>WebAspNetCore</AssemblyName>
    <RootNamespace>WebAspNetCore</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.All" Version="2.2.8" />
    <PackageReference Include="Prometheus.Client.AspNetCore" Version="4.1.0" />
    <PackageReference Include="Prometheus.Client" Version="4.2.0" />
    <PackageReference Include="Prometheus.Client.DependencyInjection" Version="0.2.0" />
  </ItemGroup>
  <ItemGroup>
    <Content Update="appsettings.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
</Project>
